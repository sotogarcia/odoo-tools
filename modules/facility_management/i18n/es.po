"            </td>\n"
"            <td style=\"padding: 3px 6px; border: 1px solid #CCCCCC; vertical-align: top;\">\n"
"                % if row['manager_email']:\n"
"                <a href=\"mailto:${ row['manager_email'] }\" target=\"_self\" style=\"text-decoration: none; color: "
"inherit; display: block;\" title=\"Send an email\">\n"
"                    ${ row['manager'] }\n"
"                </a>\n"
"                % else:\n"
"                    ${ row['manager'] }\n"
"                % endif\n"
"                % if row['manager_phone']:\n"
"                <a href=\"tel:${ row['manager_phone'] }\" target=\"_self\" style=\"font-size: smaller; text-decoration: "
"none; color: #999999; display: block;\" title=\"Call by phone\">\n"
"                    ${ row['manager_phone'] }\n"
"                </a>\n"
"                % endif\n"
"            </td>\n"
"            <td style=\"padding: 3px 6px; border: 1px solid #CCCCCC; vertical-align: top; text-align: center;\" title="
