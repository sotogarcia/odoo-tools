<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data noupdate="0">

        <record id="view_ir_exports_line_tree" model="ir.ui.view">
            <field name="name">Export field lists</field>
            <field name="model">ir.exports.line</field>
            <field name="type">tree</field>
            <field name="mode">primary</field>
            <field name="priority" eval="16" />
            <field name="active" eval="True" />
            <field name="arch" type="xml">
                <tree string="Export field" create="1" delete="1" edit="1" editable="bottom">
                    <field name="name" class="oe_field_name" />
                    <field name="export_id" class="oe_field_export_id" optional="hide" />
                    <field name="create_uid" class="oe_field_create_uid" optional="hide" />
                    <field name="create_date" class="oe_field_create_date" optional="hide" />
                    <field name="write_uid" class="oe_field_write_uid" optional="hide" />
                    <field name="write_date" class="oe_field_write_date" optional="hide" />
                </tree>
            </field>
        </record>

    </data>
</openerp>